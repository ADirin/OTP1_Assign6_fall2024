 pipeline {
  agent any

  stages {
      stage('Build') {
          steps {
              sh 'javac Account.java AccountTest.java'
          }
      }
      stage('Test') {
          steps {
              sh 'java -cp .:junit-4.12.jar:hamcrest-core-1.3.jar org.junit.runner.JUnitCore AccountTest'
          }
      }
  }
  } ```
